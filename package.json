{
  "name": "etre-et-paraitre-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon --exec 'npm run build:server && node dist/server.cjs' --watch 'server/**/*' -e ts --verbose",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build:server": "esbuild server/server.ts --bundle --platform=node --outdir=dist --sourcemap --out-extension:.js=.cjs",
    "build:frontend": "vite build",
    "build": "npm run build:frontend && npm run build:server",
    "admin:setup": "node scripts/setup-admin.js"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "express": "^4.21.1",
    "history": "^5.3.0",
    "jsonwebtoken": "^9.0.2",
    "mobx-react-lite": "^4.0.7",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.2.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "mobx": "^6.13.5",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
